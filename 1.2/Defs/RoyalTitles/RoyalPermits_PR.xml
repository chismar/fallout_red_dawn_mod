	<FalloutRedScare.FRSRoyalTitlePermitDef>
		<defName>FRS_PR_StartRebellion</defName>
		<label>start rebellion</label>
		<description>start rebellion.</description>
		<workerClass>FalloutRedScare.StartRebellion</workerClass>
