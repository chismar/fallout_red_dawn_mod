	<FalloutRedScare.FRSRoyalTitlePermitDef>
		<defName>FRS_PR_ShuttleSquadAirdrop</defName>
		<label>shuttle squad airdrop</label>
		<description>shuttle squad airdrop.</description>
		<workerClass>FalloutRedScare.CallAirSupport</workerClass>
		<workerSettings Class="FalloutRedScare.AirSupportSettings"> 
			<shuttleDef>FRS_Shuttle</shuttleDef>
			<shuttleSkyfallerIncoming>FRS_ShuttleIncoming</shuttleSkyfallerIncoming>
			<pawnGroupMakers>
				<li Class="FalloutRedScare.PawnGroupMakerPR">
					<kindDef>Combat</kindDef>
					<commonality>100</commonality>
					<options>
            <FRS_PR_RedArmyConscriptSummer>2</FRS_PR_RedArmyConscriptSummer>
            <FRS_PR_RedArmyConscriptWinter>2</FRS_PR_RedArmyConscriptWinter>
            <FRS_PR_RedArmyConscriptSummerWithMask>2</FRS_PR_RedArmyConscriptSummerWithMask>
            <FRS_PR_RedArmyConscriptWinterWithMask>2</FRS_PR_RedArmyConscriptWinterWithMask>
            <FRS_PR_RedArmyMilitant>5</FRS_PR_RedArmyMilitant>
            <FRS_PR_RedArmyOfficer>1</FRS_PR_RedArmyOfficer>
            <FRS_PR_StealthDragoon>2</FRS_PR_StealthDragoon>
            <FRS_PR_PowerArmorSoldier>2</FRS_PR_PowerArmorSoldier>
					</options>
				</li>
			</pawnGroupMakers>
			<points>5000</points>
		</workerSettings>
		<minTitle>FRS_PartyCommitte</minTitle>
		<faction>FRS_PR</faction>
		<permitPointCost>1</permitPointCost>
		<uiPosition>(0,0)</uiPosition>
		<cooldownDays>60</cooldownDays>
	</FalloutRedScare.FRSRoyalTitlePermitDef>
	
	<FalloutRedScare.FRSRoyalTitlePermitDef>
		<defName>FRS_PR_AirBombingLiberators</defName>
		<label>liberator bombing bombing</label>
		<description>liberator bombing.</description>
		<workerClass>FalloutRedScare.AirBombing</workerClass>
		<workerSettings Class="FalloutRedScare.AirBombingSettings"> 
			<airBombingCommandKey>FRS.AirBombingLabelLiberators</airBombingCommandKey>
			<shootingLineAngle>0</shootingLineAngle>
			<skyfallerBomber>FRS_ShuttleBomberLeaving</skyfallerBomber>
			<shells>
				<li>FRS_Bullet_Shell_Liberator</li>
			</shells>
      <shellsCount>3</shellsCount>
      <bombingRunAroundTargetMeters>-10~10</bombingRunAroundTargetMeters>
		</workerSettings>
		<minTitle>FRS_PartyCommitte</minTitle>
		<faction>FRS_PR</faction>
		<permitPointCost>1</permitPointCost>
		<uiPosition>(0,1)</uiPosition>
    <cooldownDays>60</cooldownDays>
  </FalloutRedScare.FRSRoyalTitlePermitDef>
  
	<FalloutRedScare.FRSRoyalTitlePermitDef>
		<defName>FRS_PR_AirBombing</defName>
