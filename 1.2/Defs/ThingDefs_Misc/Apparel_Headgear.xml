<Defs>

  <ThingDef ParentName="HatMakeableBase">
    <defName>FRS_Apparel_RedArmyWinterHeadgear</defName>
    <label>red army winter headgear</label>
    <description>red army winter headgear</description>
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>FRS/Pawn/Humanlike/Headgear/RedArmyWinterHeadgear/TrapperHat</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(0.9, 0.9)</drawSize>
    </graphicData>
    <costStuffCount>25</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToMake>1800</WorkToMake>
      <Mass>0.14</Mass>
      <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.10</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.50</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>0.8</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <SocialImpact>0.10</SocialImpact>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>FRS/Pawn/Humanlike/Headgear/RedArmyWinterHeadgear/TrapperHat</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>IndustrialBasic</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
    </apparel>
