<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="HatMakeableBase">
    <defName>FRS_Apparel_RedArmyWinterHeadgear</defName>
    <label>PLA winter hat</label>
    <description>Standard issue PLA winter hat. Doesn't protect the face very much. Warm. Reinforced with steel.</description>
    <recipeMaker>
      <researchPrerequisite>FRS_PLA_StandardIssueGear</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>FRS/Pawn/Humanlike/Headgear/RedArmyWinterHeadgear/RedArmyWinterHeadgear</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(0.9, 0.9)</drawSize>
    </graphicData>
    <costList>
      <Steel>50</Steel>
    </costList>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToMake>1800</WorkToMake>
      <Mass>0.14</Mass>
      <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.10</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.50</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>0.8</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <SocialImpact>0.10</SocialImpact>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>FRS/Pawn/Humanlike/Headgear/RedArmyWinterHeadgear/RedArmyWinterHeadgear</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>RedArmy</li>
        <li>Conscript</li>
        <li>RedArmyConscript</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
    </apparel>

  </ThingDef>
