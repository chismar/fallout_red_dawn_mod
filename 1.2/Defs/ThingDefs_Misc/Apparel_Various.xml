<?xml version="1.0" encoding="utf-8" ?>
<Defs>



  <ThingDef ParentName="ArmorMachineableBase">
    <defName>FRS_Apparel_RedArmyJumpsuit</defName>
    <label>red army jumpsuit</label>
    <description>red army jumpsuit</description>
    <recipeMaker>
      <researchPrerequisite>FlakArmor</researchPrerequisite>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>4</Crafting>
      </skillRequirements>
    </recipeMaker>
    <graphicData>
      <texPath>FRS/Pawn/Humanlike/Apparel/RedArmyJumpsuit/Jumpsuit</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.00</drawSize>
    </graphicData>
    <costList>
      <Cloth>50</Cloth>
      <Steel>70</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <thingCategories>
      <li>ApparelArmor</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>200</MaxHitPoints>
      <WorkToMake>14000</WorkToMake>
      <Mass>7</Mass>
      <Flammability>0.6</Flammability>
      <ArmorRating_Sharp>0.40</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.08</ArmorRating_Blunt>
      <ArmorRating_Heat>0.10</ArmorRating_Heat>
      <Insulation_Cold>14.4</Insulation_Cold>      <!-- like a cloth jacket -->
      <Insulation_Heat>3</Insulation_Heat>
      <EquipDelay>3</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>-0.12</MoveSpeed>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
      </bodyPartGroups>
      <wornGraphicPath>FRS/Pawn/Humanlike/Apparel/RedArmyJumpsuit/Jumpsuit</wornGraphicPath>
      <layers>
        <li>Shell</li>
      </layers>
      <tags>
        <li>IndustrialAdvanced</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <only>(0.44,0.44,0.44)</only>
        </li>
        <li>
          <only>(0.62,0.62,0.62)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>
